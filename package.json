{
  "name": "health-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.4",
    "chai": "^4.2.0",
    "echarts": "^4.4.0",
    "echarts-for-react": "2.0.15-beta.1",
    "echartslayer": "^0.1.6",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.1",
    "mapbox-gl": "^1.4.1",
    "mocha": "^6.2.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "subscriptions-transport-ws": "^0.9.16",
    "web3": "^1.2.2"
  },
  "scripts": {
    "start": "concurrently \"node mock/index.js\" \"npm run start:react\"",
    "start:react": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run test:react",
    "test:react": "CI=true react-scripts test --passWithNoTests",
    "lint": "eslint ./src/**/*.jsx",
    "coverage": "",
    "format": "pretty-quick --staged --pattern \"src/**/*.*(js|jsx)\"",
    "deploy": "git push gh `git subtree split --prefix build master`:master --force"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "husky": "^3.0.8",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm run lint && npm run format"
    }
  }
}

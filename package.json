{
  "name": "health-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "chai": "^4.2.0",
    "echarts": "^4.4.0",
    "echartslayer": "^0.1.6",
    "mapbox-gl": "^1.4.1",
    "mocha": "^6.2.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
